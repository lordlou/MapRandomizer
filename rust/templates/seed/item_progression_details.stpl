<div class="row">
    <div class="col-3">Progression rate:</div>
    <div class="col-9"><%= progression_rate %></div>
</div>
<div class="row">
    <div class="col-3">Item placement:</div>
    <div class="col-9"><%= item_placement_style %></div>
</div>
<% for group in &difficulty.item_priorities { %>
    <% if group.name != "Default" && group.items.len() > 0 { %>
        <div class="row">
            <div class="col-3"><%= group.name %> priority:</div>
            <div class="col-9">
                <%= group.items.join(", ") %>
            </div>
        </div>
    <% } %>
<% } %>
<div class="row">
    <div class="col-3">Filler items:</div>
    <div class="col-9">
        <%= filler_items.join(", ") %>
    </div>
</div>
<% if early_filler_items.len() > 0 { %>
<div class="row">
    <div class="col-3">Early filler items:</div>
    <div class="col-9">
        <%= early_filler_items.join(", ") %>
    </div>
</div>
<% } %>